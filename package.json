{
  "name": "hbll-model-viewer",
  "version": "1.0.0",
  "description": "Web component for the HBLL model viewer",
  "main": "dist/hbll-model-viewer-umd.js",
  "module": "dist/hbll-model-viewer.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-old": "npm run build:tsc && npm run build:rollup",
    "build": "webpack",
    "build:rollup": "rollup -c --environment NODE_ENV:production",
    "build:tsc": "tsc --incremental",
    "serve": "concurrently \"http-server -a localhost -p 8080 .\" \"sleepms 1000 && start chrome http://localhost:8080/example\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HBLL-Experiential-Lab/hbll-model-viewer-library.git"
  },
  "author": "Ben Brenkman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HBLL-Experiential-Lab/hbll-model-viewer-library/issues"
  },
  "homepage": "https://github.com/HBLL-Experiential-Lab/hbll-model-viewer-library#readme",
  "dependencies": {
    "@google/model-viewer": "^1.3.0",
    "@ts-stack/markdown": "^1.3.0",
    "http-server": "^0.12.3",
    "lit-element": "^2.4.0",
    "material-components-web": "^8.0.0",
    "rollup-plugin-import-css": "^1.1.2",
    "three": "^0.122.0"
  },
  "devDependencies": {
    "@google/model-viewer": "^1.3.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@types/file-saver": "^2.0.1",
    "concurrently": "^5.3.0",
    "lit-html": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.26.6",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-polyfill": "^3.0.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-terser": "^7.0.0",
    "sleep-ms": "^2.0.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.5",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "keywords": [
    "gltf",
    "glb",
    "webar",
    "webvr",
    "webxr",
    "model-viewer",
    "3d",
    "hbll"
